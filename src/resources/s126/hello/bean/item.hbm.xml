<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

	<class name="s126.hello.bean.Item" table="items_info">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name" type="string" length="30" ></property>
		<property name="price" type="float"></property>
		<property name="cdate" type="java.util.Date" column="ctime" ></property>
		<property name="status" type="int"></property>
		
	</class>
	
	
</hibernate-mapping>
